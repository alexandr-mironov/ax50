+ *          handle the error condition and return error
+ ***************************************************************************/
+int ti_dev_new_index(struct net *net)
+{
+	int ifindex = 0;
+
+    while (ifindex++ < TI_MAX_DEVICE_INDEX)
+    {
+		if (!__dev_get_by_index(net,ifindex))
+			return ifindex;
+	}
+    printk ("Error: Failed to allocate netdevice index\n");
+    return -1;
+}
